number_1 = int(input("Enter the first number you want to use: "))
print(number_1)
number_2 = int(input("Enter the second number you want to use: "))
print(number_2)
print("Here are some math expressions with these two numbers:")
addition = number_1 + number_2
subtraction = number_1 - number_2
multiplication = number_1 * number_2
division = number_1 / number_2
print(addition, subtraction, multiplication, division)
